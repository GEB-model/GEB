    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Spinup",
            "type": "debugpy",
            "request": "launch",
            "program": "geb/cli.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "args": [
                "spinup",
                "-wd",
                "../models/geul/base",
            ],
        },
        {
            "name": "Run",
            "type": "debugpy",
            "request": "launch",
            "program": "geb/cli.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "args": [
                "run",
                "-wd",
                "../models/geul/base"
            ]
        },
        {
            "name": "Build",
            "type": "debugpy",
            "request": "launch",
            "program": "geb/cli.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "args": [
                "build",
                "-wd", "../models/geul/base",
            ]
        },
        {
            "name": "Update",
            "type": "debugpy",
            "request": "launch",
            "program": "geb/cli.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "args": [
                "update",
                "-wd", "../models/geul/base",
            ]
